{
  "name": "emag/cache-bundle",
  "description": "CacheBundle by eMAGTechLabs",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandru Bumbacea",
      "email": "alexandru.bumbacea@emag.ro"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ocramius/proxy-manager": "@stable",
    "psr/cache": "1.0.*",
    "doctrine/annotations": "@stable"
  },
  "suggest": {
    "symfony/cache": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "2.*",
    "phpmd/phpmd" : "@stable",
    "phing/phing": "@stable",
    "sebastian/phpcpd": "2.0.2",
    "symfony/cache": "3.*",
    "satooshi/php-coveralls": "~1.0",
    "symfony/monolog-bundle": "@stable",
    "symfony/framework-bundle": "@stable"
  },
  "autoload": {
    "psr-4": { "eMAG\\CacheBundle\\": "" },
    "psr-0": { "eMAG\\CacheBundle\\Annotation\\": "" }
  },
  "config": {
    "optimize-autoloader": true
  }
}
