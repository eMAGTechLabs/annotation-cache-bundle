{
  "name": "emag/cache-bundle",
  "description": "CacheBundle by eMAGTechLabs",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandru Bumbacea",
      "email": "alexandru.bumbacea@emag.ro"
    }
  ],
  "require": {
    "php": ">=5.5",
    "jms/aop-bundle": "1.*",
    "symfony/monolog-bundle": "~2.9"
  },
  "suggest": {
    "predis/predis": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "squizlabs/php_codesniffer": "2.*",
    "phpmd/phpmd" : "@stable",
    "phing/phing": "@stable",
    "sebastian/phpcpd": "2.0.2",
    "predis/predis": "1.0.*",
    "satooshi/php-coveralls": "~1.0",
    "ext-memcached": "*",
    "ext-couchbase": "*"
  },
  "autoload": {
    "psr-4": { "CacheBundle\\": "src/CacheBundle" },
    "psr-0": { "CacheBundle\\Annotation": "src/" }
  },
  "config": {
    "optimize-autoloader": true
  }
}
